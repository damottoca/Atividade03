public class Main {
    public static void main(String[] args) {

        class Vetores01 {
            public static void main(String[] args) {
                // Declaração e inicialização do vetor
                int[] vetor = {10, 20, 30, 40, 50};

                // Acesso ao terceiro elemento (índice 2, pois os índices começam em 0)
                int terceiroElemento = vetor[2];

                // Exibição do terceiro elemento
                System.out.println("O terceiro elemento é: " + terceiroElemento);
            }
        }
    }
}
